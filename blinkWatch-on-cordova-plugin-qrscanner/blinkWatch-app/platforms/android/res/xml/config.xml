<?xml version='1.0' encoding='utf-8'?>
<widget id="cw.swcore.ceruleanplugins-blinkwatchapp" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BlinkWatch">
        <param name="android-package" value="cw.swcore.ceruleanplugins.BlinkWatch" />
    </feature>
    <name>blinkWatchApp</name>
    <description>
     A tester app for blinkWatch plugin from Cerulean Plugins suite.
    </description>
    <author email="mzhukov314154dev@gmail.com" href="">
     Maxim Zhukov
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
